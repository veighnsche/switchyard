{
  "functions": {
    "plan": {
      "axes": {
        "link_count": ["min:0", "one", "many:10", "huge:1000"],
        "restore_count": ["min:0", "one", "many:10"],
        "duplicate_targets": ["false", "true"],
        "input_order": ["sorted", "reverse", "random:seed=42"]
      },
      "constraints": [],
      "risk_overrides": {}
    },
    "preflight": {
      "axes": {
        "rescue.require": ["false", "true"],
        "rescue.exec_check": ["false", "true"],
        "rescue.min_count": ["min:0", "nominal:1", "huge:100"],
        "risks.ownership_strict": ["false", "true"],
        "durability.preservation": ["Off", "RequireBasic"],
        "apply.extra_mount_checks.count": ["min:0", "one", "many:5"],
        "backup.tag": ["empty", "coreutils", "long:256"]
      },
      "constraints": [
        "IF risks.ownership_strict=true THEN ownership_oracle IN {present}",
        "IF rescue.require=true AND rescue.min_count>0 THEN rescue_tools_available=true"
      ],
      "risk_overrides": {
        "rescue.require": "High",
        "risks.ownership_strict": "High",
        "durability.preservation": "High"
      }
    },
    "apply": {
      "axes": {
        "mode": ["DryRun", "Commit"],
        "governance.locking": ["Optional", "Required"],
        "lock_manager": ["present", "absent"],
        "lock_timeout_ms": ["min:0", "nominal:100", "max:10000"],
        "apply.override_preflight": ["false", "true"],
        "apply.exdev": ["Fail", "DegradedFallback"],
        "apply.best_effort_restore": ["false", "true"],
        "apply.capture_restore_snapshot": ["false", "true"],
        "governance.smoke": ["Off", "Require:auto_rollback=false", "Require:auto_rollback=true"],
        "smoke_runner": ["present", "absent"],
        "smoke_outcome": ["ok", "fail"],
        "risks.ownership_strict": ["false", "true"],
        "ownership_oracle": ["present", "absent"],
        "attestor": ["present", "absent"],
        "attestation_outcome": ["ok", "error"]
      },
      "constraints": [
        "IF governance.locking=Required AND mode=Commit THEN lock_manager=present",
        "IF governance.smoke IN {Require:auto_rollback=false, Require:auto_rollback=true} AND mode=Commit THEN smoke_runner=present",
        "IF mode=DryRun THEN smoke_outcome=ok AND attestation_outcome=ok",
        "IF risks.ownership_strict=true AND apply.override_preflight=false THEN preflight_ok=true"
      ],
      "risk_overrides": {
        "mode": "High",
        "governance.locking": "High",
        "apply.exdev": "High",
        "governance.smoke": "High",
        "apply.best_effort_restore": "High",
        "risks.ownership_strict": "High"
      }
    },
    "plan_rollback_of": {
      "axes": {
        "apply.capture_restore_snapshot": ["false", "true"],
        "apply_report.executed_shape": ["only_symlink", "only_restore", "mixed"]
      },
      "constraints": [],
      "risk_overrides": {}
    },
    "prune_backups": {
      "axes": {
        "backup.tag": ["default", "coreutils", "long:256"],
        "retention_count_limit": ["none", "min:0", "one:1", "many:5"],
        "retention_age_limit": ["none", "min:1s", "nominal:1d", "max:365d"]
      },
      "constraints": [],
      "risk_overrides": {
        "retention_count_limit": "Medium",
        "retention_age_limit": "Medium"
      }
    },
    "SafePath_from_rooted": {
      "axes": {
        "root_is_absolute": ["true", "invalid:false"],
        "candidate_form": ["relative_normal", "absolute_inside_root", "absolute_outside_root:invalid", "dotdot:invalid", "curdir_components", "unsupported_component:invalid"],
        "path_length": ["empty", "short:3segs", "long:255bytes", "huge:4096bytes"],
        "unicode": ["false", "true"]
      },
      "constraints": [
        "IF root_is_absolute=invalid:false THEN expect=panic_or_error",
        "IF candidate_form IN {absolute_outside_root:invalid, dotdot:invalid, unsupported_component:invalid} THEN expect=Error"
      ],
      "risk_overrides": {
        "root_is_absolute": "High",
        "candidate_form": "High"
      }
    }
  }
}
